{
	"name": "www",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node --env-file=.env build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun --run prettier --write .",
		"db:migrate": "bun run src/lib/server/db/scripts/migrate.ts",
		"db:backup": "bun run scripts/backup-db.ts",
		"db:restore": "bun run scripts/restore-db.ts",
		"test:integration": "NODE_ENV=test pnpm run db:test:init && pnpm run db:test:seed && pnpm run build && playwright test",
		"test": "vitest"
	},
	"devDependencies": {
		"@orama/orama": "^3.1.14",
		"@playwright/test": "^1.55.1",
		"@sveltejs/adapter-node": "^5.3.3",
		"@sveltejs/enhanced-img": "^0.6.1",
		"@sveltejs/kit": "^2.44.0",
		"@sveltejs/vite-plugin-svelte": "5.1.0",
		"@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.14",
		"@thisux/sveltednd": "^0.0.20",
		"@types/bun": "^1.2.23",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^2.11.4",
		"dotenv": "^16.6.1",
		"formsnap": "^2.0.1",
		"magnitude-test": "^0.1.4",
		"marked": "^15.0.12",
		"phosphor-svelte": "^3.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svead": "^0.0.13",
		"svelte": "^5.39.9",
		"svelte-check": "^4.3.2",
		"svelte-sonner": "^1.0.5",
		"sveltekit-search-params": "^3.0.0",
		"sveltekit-superforms": "^2.27.2",
		"tailwindcss": "^4.1.14",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"vite": "^6.3.6",
		"vitest": "^3.2.4",
		"@ai-sdk/anthropic": "^1.2.12",
		"ai": "^4.3.19",
		"@epic-web/cachified": "^5.6.0",
		"zod": "^3.25.76"
	},
	"dependencies": {
		"duckdb-async": "^1.4.0"
	},
	"type": "module"
}
