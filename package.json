{
	"name": "www",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node --env-file=.env build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun --run prettier --write .",
		"db:migrate": "bun run src/lib/server/db/scripts/migrate.ts",
		"db:backup": "bun run scripts/backup-db.ts",
		"db:restore": "bun run scripts/restore-db.ts",
		"db:restore:latest": "bun run scripts/restore-latest-from-s3.ts",
		"test:integration": "NODE_ENV=test pnpm run db:test:init && pnpm run db:test:seed && pnpm run build && playwright test",
		"test": "vitest"
	},
	"devDependencies": {
		"@ai-sdk/anthropic": "^2.0.27",
		"@epic-web/cachified": "^5.6.0",
		"@orama/orama": "^3.1.15",
		"@playwright/test": "^1.56.0",
		"@sveltejs/adapter-node": "^5.3.3",
		"@sveltejs/enhanced-img": "^0.8.4",
		"@sveltejs/kit": "^2.46.4",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@sveltejs/vite-plugin-svelte-inspector": "^5.0.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.14",
		"@thisux/sveltednd": "^0.0.20",
		"@types/bun": "^1.3.0",
		"ai": "^4.3.19",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^2.11.5",
		"dotenv": "^17.2.3",
		"formsnap": "^2.0.1",
		"magnitude-test": "^0.3.12",
		"marked": "^16.4.0",
		"phosphor-svelte": "^3.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svead": "^0.0.15",
		"svelte": "^5.39.11",
		"svelte-adapter-bun": "^1.0.0",
		"svelte-check": "^4.3.3",
		"svelte-sonner": "^1.0.5",
		"sveltekit-search-params": "^3.0.0",
		"sveltekit-superforms": "^2.27.2",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.14",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3",
		"vite": "^7.1.9",
		"vitest": "^3.2.4",
		"zod": "^4.1.12"
	},
	"type": "module"
}
