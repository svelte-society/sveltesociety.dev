{
	"name": "sveltesociety.dev",
	"repository": "https://github.com/svelte-society/sveltesociety.dev.git",
	"packageManager": "pnpm@9.2.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"start": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"test": "vitest",
		"lint": "prettier --check '**/*' --ignore-unknown && eslint . && node scripts/validateData.js",
		"format": "prettier --write '**/*' --ignore-unknown",
		"update:licenses": "pnpm licenses list --long --json > src/routes/licenses/licenses.json && node scripts/licenses.mjs && prettier --write 'src/routes/licenses/licenses.json'"
	},
	"devDependencies": {
		"@macfja/svelte-persistent-store": "2.4.1",
		"@sveltejs/adapter-auto": "3.2.1",
		"@sveltejs/kit": "2.5.10",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/eslint": "8.56.10",
		"@types/node": "20.14.2",
		"@typescript-eslint/eslint-plugin": "6.16.0",
		"@typescript-eslint/parser": "6.16.0",
		"autoprefixer": "10.4.19",
		"dayjs": "1.11.11",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.39.0",
		"get-npm-tarball-url": "2.1.0",
		"highlight.js": "11.9.0",
		"mdsvex": "0.11.2",
		"package-name-regex": "4.0.0",
		"postcss": "8.4.38",
		"prettier": "3.3.1",
		"prettier-plugin-svelte": "3.2.4",
		"prettier-plugin-tailwindcss": "0.6.2",
		"publint": "0.3.2",
		"rehype-slug": "6.0.0",
		"svelte": "4.2.18",
		"svelte-check": "3.8.0",
		"svelte-highlight": "7.4.7",
		"svelte-select": "4.4.7",
		"tailwindcss": "3.4.4",
		"typescript": "5.3.3",
		"vite": "5.2.13",
		"vitest": "1.6.0",
		"zod": "3.23.8"
	}
}
