{
	"name": "www",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node --env-file=.env build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun --run prettier --write .",
		"db:migrate": "bun run src/lib/server/db/scripts/migrate.ts",
		"db:backup": "bun run scripts/backup-db.ts",
		"test:integration": "NODE_ENV=test pnpm run db:test:init && pnpm run db:test:seed && pnpm run build && playwright test",
		"test": "vitest"
	},
	"devDependencies": {
		"@orama/orama": "^3.1.10",
		"@playwright/test": "^1.53.2",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/enhanced-img": "^0.6.1",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "5.1.0",
		"@sveltejs/vite-plugin-svelte-inspector": "^4.0.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@thisux/sveltednd": "^0.0.20",
		"@types/bun": "^1.2.18",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^2.8.10",
		"dotenv": "^16.6.1",
		"formsnap": "^2.0.1",
		"magnitude-test": "^0.1.4",
		"marked": "^15.0.12",
		"phosphor-svelte": "^3.0.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"svead": "^0.0.13",
		"svelte": "^5.35.2",
		"svelte-check": "^4.2.2",
		"svelte-sonner": "^1.0.5",
		"sveltekit-search-params": "^3.0.0",
		"sveltekit-superforms": "^2.27.1",
		"tailwindcss": "^4.1.11",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vitest": "^3.2.4",
		"@ai-sdk/anthropic": "^1.2.12",
		"ai": "^4.3.16",
		"@epic-web/cachified": "^5.6.0",
		"zod": "^3.25.73"
	},
	"dependencies": {
		"duckdb-async": "^1.3.1"
	},
	"type": "module"
}
