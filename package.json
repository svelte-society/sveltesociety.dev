{
	"name": "www",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node --env-file=.env build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "bun --run prettier --write .",
		"db:init": "NODE_ENV=development bun ./src/lib/server/db/initiate.ts",
		"db:seed": "NODE_ENV=development bun ./src/lib/server/db/seeds/index.ts",
		"db:test:init": "NODE_ENV=test bun ./src/lib/server/db/initiate.ts",
		"db:test:seed": "NODE_ENV=test bun ./src/lib/server/db/seeds/index.ts",
		"test:integration": "NODE_ENV=test pnpm run db:test:init && pnpm run db:test:seed && pnpm run build && playwright test",
		"test": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "^1.51.1",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.17",
		"@types/bun": "^1.2.6",
		"autoprefixer": "^10.4.21",
		"bits-ui": "^1.3.13",
		"dotenv": "^16.4.7",
		"formsnap": "^2.0.0",
		"marked": "^15.0.7",
		"package-name-regex": "^4.0.0",
		"phosphor-svelte": "^3.0.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"svelte-headlessui": "^0.0.46",
		"sveltekit-search-params": "^3.0.0",
		"sveltekit-superforms": "^2.24.0",
		"tailwindcss": "^4.0.17",
		"tslib": "^2.8.1",
		"typescript": "^5.8.2",
		"vite": "^6.2.3",
		"vitest": "^3.0.9",
		"zod": "^3.24.2"
	},
	"dependencies": {
		"duckdb-async": "^1.2.0"
	},
	"type": "module"
}
